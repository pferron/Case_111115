  "scripts": {
    "android": "react-native run-android",
    "android:build": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease",
    "android:build:clean:production": "npm run android:clean && npm run android:build:production:bundle",
    "android:build:clean:staging": "npm run android:clean && npm run android:build:staging",
    "android:build:production": "cd android && ./gradlew assembleProductionRelease",
    "android:build:production:aab": "cd android && ./gradlew bundleProductionRelease",
    "android:build:production:apks": "cd android && ./gradlew buildProductionApks",
    "android:build:production:bundle": "npm run android:build:production:aab && npm run android:build:production:apks",
    "android:build:staging": "cd android && ./gradlew assembleStagingRelease",
    "android:clean": "cd android && rm -rf .gradle && ./gradlew clean",
    "android:clean:build": "npm run android:clean && npm run android:build",
    "android:db": "./scripts/opendb.js --o --android --d",
    "android:db:pull": "./scripts/opendb.js --android --d",
    "android:db:push": "./scripts/pushdb.android.js --android",
    "android:dbd": "./scripts/opendb.js --o --android --d --device",
    "android:dbd:pull": "./scripts/opendb.js --android --d --device",
    "android:dbd:push": "./scripts/pushdb.android.js --android --d --device",
    "android:pr": "./scripts/installpr.js --android",
    "apply-patch": "patch-package",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "watchman watch-del-all && rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf whitesource",
    "cleani": "npm run clean && npm install",
    "commitlint:last": "commitlint --from=HEAD~1",
    "copy-db": "nodemon  --watch ../svmx-db/lib  --exec \"mkdir -p js/svmx-db && cp -R ../svmx-db/lib/* js/svmx-db && mv js/svmx-db/index.js js/svmx-db/index.bck.js && mv js/svmx-db/index.rn.js js/svmx-db/index.js\"",
    "copy-db2": "nodemon  --watch ../svmx-db/src  --exec \"mkdir -p js/svmx-db && cp -R ../svmx-db/src/* js/svmx-db && mv js/svmx-db/index.js js/svmx-db/index.bck.js && mv js/svmx-db/index.rn.js js/svmx-db/index.js\"",
    "copy-engine": "nodemon   --watch ../sfm-engine/src  --ignore \"../sfm-engine/src/*.test.js\" --exec \"mkdir -p js/@svmx/sfm-engine && cp -R ../sfm-engine/src/* js/@svmx/sfm-engine \"",
    "copy-fetch": "nodemon  --watch ../react-native-fetch-blob  --exec \"mkdir -p node_modules/react-native-fetch-blob && rsync -av --exclude='node_modules/react-native/Libraries' --exclude='node_modules/react-native/package.json' ../react-native-fetch-blob/* node_modules/react-native-fetch-blob/ \"",
    "copy-sync": "nodemon  --watch ../sync-client/src  --exec \"mkdir -p js/@svmx/sync-client && cp -R ../sync-client/src/* js/@svmx/sync-client \"",
    "copy-sync-native-ios:back": "nodemon  --watch node_modules/@svmx/sync-client-native-ios/ios  --exec \"cp -R node_modules/@svmx/sync-client-native-ios/ios/* ../sync-client-native-ios/ios  \"",
    "copy-sync-native-ios:origin": "nodemon  --watch ../sync-client-native-ios/ios  --exec \"mkdir -p node_modules/@svmx/sync-client-native-ios/ios && cp -R ../sync-client-native-ios/ios/* node_modules/@svmx/sync-client-native-ios/ios \"",
    "copyright": "./scripts/copyright.js",
    "cov": "jest --coverage",
    "cov:open": "open coverage/lcov-report/index.html",
    "cov:watch": "npm run cov -- --watchAll",
    "covc": "jest --runInBand --coverage --no-cache",
    "fixpack": "fixpack",
    "flow": "flow",
    "flow-scripts": "flow-scripts stub",
    "flow-typed": "rm -rf ./flow-typed/npm && flow-typed install",
    "flow:watch": "flow-watch",
    "formula": "./scripts/formula.js",
    "generate-native-storybook": "rnstl --searchDir ./js --pattern \"**/*.stories.js\" --outputFile ./js/storybook/storyLoader.js",
    "i18n-discover": "./scripts/i18n-discover.js",
    "i18n-extract": "./scripts/i18n-extract.js",
    "i18n-merge": "./scripts/i18n-merge.js",
    "i18n-verify": "./scripts/i18n-verify.js",
    "ios": "react-native run-ios",
    "ios:build": "cd ios && bundle exec fastlane automation",
    "ios:build:dev": "cd ios && bundle exec fastlane dev",
    "ios:build:dist_universal": "npm run ios:clean && cd ios && bundle exec fastlane dist_universal",
    "ios:build:distribution": "npm run ios:clean && cd ios && bundle exec fastlane dist",
    "ios:build:production": "cd ios && bundle exec fastlane production",
    "ios:build:staging": "cd ios && bundle exec fastlane staging",
    "ios:clean": "rm -rf ios/build",
    "ios:db": "./scripts/opendb.js --o --ios --d",
    "ios:db:pull": "./scripts/opendb.js --ios --d",
    "ios:db:push": "./scripts/pushdb.ios.js",
    "ios:dbd": "./scripts/opendb.js --o --ios --d --device",
    "ios:dbd:pull": "./scripts/opendb.js --ios --d --device",
    "ios:dbd:push": "./scripts/pushdb.ios.js --device",
    "ios:device": "path-exists ios/build/FSARN-Debug.ipa || npm run ios:build:dev && ipa-deploy ios/build/FSARN-Debug.ipa",
    "ios:pr": "./scripts/installpr.js --ios",
    "ipad": "react-native run-ios --simulator=\"iPad Air 2\"",
    "ipad11": "react-native run-ios --simulator=\"iPad Pro (11-inch)\"",
    "iphone": "react-native run-ios --scheme FSARN --simulator=\"iPhone 7\"",
    "iphone:release": "react-native run-ios --simulator=\"iPhone 7\" --configuration=Release",
    "jest:clean": "jest --clearCache && rm -rf coverage",
    "lint": "eslint js test *.js",
    "lint:fix": "eslint --fix js test *.js && node ./scripts/copyright.js",
    "lint:report": "npm run lint:report:checkstyle",
    "lint:report:checkstyle": "eslint -f checkstyle -o ./lint/eslint.xml js test *.js",
    "lint:report:html": "eslint -f html -o ./lint/eslint.html js test *.js",
    "log": "./scripts/openlog.js",
    "log:device": "./scripts/openlog.js --device",
    "object-update": "./scripts/object-update.js",
    "p": "npm run lint:fix",
    "postinstall": "./postinstall.sh",
    "prebuild": "rollup --config && cat ./node_modules/moment/min/moment-with-locales.js ./assets/opdoc-helper.js > ./assets/opdoc-helper1.js && mv ./assets/opdoc-helper1.js ./assets/opdoc-helper.js && ./setup-dataguide-form-web-app.sh",
    "preinstall": "npx npm-force-resolutions",
    "prepublishOnly": "./prepublish.sh",
    "prestart": "npm run generate-native-storybook",
    "prestartc": "npm run generate-native-storybook",
    "save-job": "nodemon  --watch ./node_modules/react-native-background-job  --exec \"rsync -vrtu ./node_modules/react-native-background-job/* ../react-native-background-job/ \"",
    "sfm:ios": "./scripts/opensfm.ios.js",
    "slather": "cd ./ios && slather coverage -s --scheme FSARN FSARN.xcodeproj",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "startc": "node -max-old-space-size=4096 node_modules/react-native/local-cli/cli.js start --reset-cache",
    "storybook": "npm run storybook:web",
    "storybook:web": "start-storybook -p 7007",
    "test": "jest --logHeapUsage",
    "test:watch": "npm run test -- --watch -- --logHeapUsage",
    "test:watchc": "npm run testc -- --watch",
    "testc": "jest --detectLeaks --no-cache --runInBand --logHeapUsage",
    "uiautomation:run:android": "cd ui-automation && ./buildScripts/build.sh && ./buildScripts/test-Android.sh",
    "uiautomation:run:ios": "cd ui-automation && ./buildScripts/build.sh && ./buildScripts/test-iOS.sh",
    "update-app-build-version": "react-native-version -A",
    "update-app-build-version-dev": "react-native-version -A . 0",
    "version": "npm run changelog && npm run update-app-build-version-dev && git add .",
    "whitesource": "java -jar wss-unified-agent.jar -c ./wss-unified-agent.config",
    "wizard:ios": "./scripts/opensfmwizard.ios.js"
  },
  "xcodeSchemes": {
    "Debug": [],
    "Release": [
      "Release-Automation"
    ],
    "projectDirectory": "iOS",
    "settings": {
      "fix-script": {
        "env": {
          "NODE_BINARY": "`which node`"
        }
      }
    }
  }
}
